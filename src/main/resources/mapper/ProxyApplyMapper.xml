<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhongjian.webserver.mapper.ProxyApplyMapper">
	<select id="queryProxyApplyCurStatus" resultType="java.lang.Integer">
		SELECT
		CurStatus from tb_proxyapply WHERE UserId = #{UserId,jdbcType=INTEGER}
	</select>
	<select id="queryProxyApply" resultType="ProxyApply">
		SELECT Name,Phone,ProvinceId,CityId,RegionId,Address,Photo1,Photo2,Photo3,CurStatus,RefuseReason
		FROM tb_proxyapply WHERE UserId = #{UserId,jdbcType=INTEGER}
	</select>
</mapper>